       return (\n            <HSMattr>\n                { capitalized(name) }\n                <Attribute isSmall={small}>\n                    <Icon \n                        isSmall={small}\n                        src={`${publicUrl}/${name}.png`} \n                        alt={`${name} icon`} \n                    />\n                    {value}\n                </Attribute>\n            </HSMattr>\n        );\n    }\n    else {\n        return (\n            <OverlayTrigger\n                placement=\"top\"\n                delay={{ show: 100, hide: 100 }}\n                overlay={<Tooltip>{capitalized(name)}</Tooltip>}\n            >\n                <Attribute isSmall={small}>\n                    <Icon \n                        isSmall={small}\n                        src={`${publicUrl}/${name}.png`} \n                        alt={`${name} icon`} \n                    />\n                    {value}\n                </Attribute>\n            </OverlayTrigger>\n        );\n    }\n}\n"]}]}